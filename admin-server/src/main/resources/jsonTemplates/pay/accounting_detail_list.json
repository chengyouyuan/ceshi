{
  "meta": {
    "code": "accountingDetailList",
    "ajaxType": "get",
    "path": "/pay/verify/accountingDetailList",
    "type": "report",
    "title": "核销明细列表",
    "buttons": [
      {
        "name": "查询",
        "event": {
          "type": "submit",
          "ajaxType": "get",
          "path": "/pay/verify/accountingDetailList"
        }
      },
      {
        "name": "重置",
        "event": {
          "type": "reset"
        }
      },
      {
        "name": "解除暂缓",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id", "verifyStatus", "thirdPartyVerifyStatus" ],
          "path": "/pay/verify/accountingDetailRestore",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "暂缓",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id", "verifyStatus", "thirdPartyVerifyStatus" ],
          "path": "/pay/verify/accountingDetailPause",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "确认结算",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id", "verifyStatus", "thirdPartyVerifyStatus" ],
          "path": "/pay/verify/verifyByDetail",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "导出Excel",
        "event": {
          "type": "export",
          "ajaxType": "post",
          "path": "/pay/verify/accountingDetailListExport"
        }
      }
    ],
    "columns": [
      {
        "type": "selection",
        "name": "id"
      },
      {
        "name": "verifyStatusName",
        "title": "结算状态"
      },
      {
        "name": "storeName",
        "title": "门店名称"
      },
      {
        "name": "storeId",
        "title": "门店ID"
      },
      {
        "name": "recordedTime",
        "title": "订单完成时间"
      },
      {
        "name": "recordedTime",
        "title": "订单入账时间"
      },
      {
        "name": "thirdPartyVerifyStatusName",
        "title": "微信入账"
      },
      {
        "name": "orderNo",
        "title": "关联订单"
      },
      {
        "name": "detailTypeName",
        "title": "费用类型"
      },
      {
        "name": "detailMoney",
        "title": "金额"
      },
      {
        "name": "thirdPartyFeeMoney",
        "title": "手续费"
      },
      {
        "name": "verifyTime",
        "title": "操作时间"
      },
      {
        "name": "operatedByName",
        "title": "操作人"
      }
    ],
    "params": [
      {
        "name": "verifyStatus",
        "inputModel": "select",
        "title": "结算状态",
        "options": [ {
          "label": "已结算",
          "value": "1"
        }, {
          "label": "未结算",
          "value": "0"
        } ]
      },
      {
        "name": "verifyDate",
        "inputModel": "double-date-picker",
        "title": "实际结算时间",
        "value": {}
      },
      {
        "name": "storeId",
        "inputModel": "inputDia",
        "title": "门店",
        "configKey": "/template/module/store/store_list_win.json"
      },
      {
        "name": "detailType",
        "inputModel": "select",
        "title": "费用类型",
        "options": [ {
          "label": "订单货款",
          "value": "1"
        }, {
          "label": "手续费",
          "value": "2"
        }, {
          "label": "优惠抵扣",
          "value": "3"
        } ]
      },
      {
        "name": "thirdPartyVerifyStatus",
        "inputModel": "select",
        "title": "微信入账",
        "options": [ {
          "label": "已入账",
          "value": "1"
        }, {
          "label": "未入账",
          "value": "0"
        } ]
      },
      {
        "name": "recordedDate",
        "inputModel": "double-date-picker",
        "title": "订单完成时间",
        "value": {}
      }
    ]
  }
}
