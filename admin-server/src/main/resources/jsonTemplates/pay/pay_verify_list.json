{
  "meta": {
    "code": "verifyList",
    "ajaxType": "get",
    "path": "/pay/verify/verifyList",
    "type": "report",
    "title": "门店核销列表",
    "buttons": [
      {
        "name": "查询",
        "event": {
          "type": "submit",
          "ajaxType": "get",
          "path": "/pay/verify/verifyList"
        }
      },
      {
        "name": "重置",
        "event": {
          "type": "reset"
        }
      },
      {
        "name": "核销",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "storeId", "lastRecordedTime", "verifyStatus" ],
          "path": "/pay/verify/verifyBySummary",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      }
    ],
    "columns": [
      {
        "type": "selection",
        "name": "id"
      },
      {
        "name": "storeId",
        "title": "门店ID"
      },
      {
        "name": "storeName",
        "title": "门店名称"
      },
      {
        "name": "verifyStatusName",
        "title": "结算状态"
      },
      {
        "name": "lastRecordedTime",
        "title": "订单截止日"
      },
      {
        "name": "operatedTime",
        "title": "核销日"
      },
      {
        "name": "realPay",
        "title": "实收总额"
      },
      {
        "name": "thirdPartyFee",
        "title": "手续费总额"
      },
      {
        "name": "discount",
        "title": "优惠券总额"
      },
      {
        "name": "realVerify",
        "title": "实结总额"
      },
      {
        "name": "operatedByName",
        "title": "操作人员"
      },
      {
        "name": "operated",
        "title": "操作时间"
      }
    ],
    "params": [
      {
        "name": "verifyStatus",
        "inputModel": "select",
        "title": "结算状态",
        "options": [ {
          "label": "已结算",
          "value": "1"
        }, {
          "label": "未结算",
          "value": "0"
        } ]
      },
      {
        "name": "recordedDate",
        "inputModel": "double-date-picker",
        "title": "订单截止日",
        "value": {}
      },
      {
        "name": "realPayMoney",
        "inputModel": "range",
        "title": "实收款",
        "value": {}
      },
      {
        "name": "storeId",
        "inputModel": "inputDia",
        "title": "门店",
        "configKey": "/template/module/order/order_store_list_win.json"
      },
      {
        "name": "verifyDate",
        "inputModel": "double-date-picker",
        "title": "核销日",
        "value": {}
      },
      {
        "name": "realVerifyMoney",
        "inputModel": "range",
        "title": "实结款",
        "value": {}
      }
    ]
  }
}
