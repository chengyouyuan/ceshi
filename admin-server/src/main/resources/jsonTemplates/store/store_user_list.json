{
  "meta": {
    "ajaxType": "post",
    "path": "/store/findStoreList",
    "type": "report",
    "id": "store_user_list",
    "code": "store_user_list",
    "title": "门店管理",
    "buttons": [
      {
        "name": "查询",
        "icon": null,
        "event": {
          "type": "submit",
          "ajaxType": "post",
          "path": "/store/findStoreList"
        }
      },
      {
        "name": "重置",
        "icon": null,
        "event": {
          "type": "reset",
          "path": ""
        }
      }
    ],
    "columns": [
      {
        "type": "index",
        "name": "id",
        "title": "序号",
        "aglign": "center",
        "width": "50",
        "fixed": "left"
      },
      {
        "name": "storeStatusDesc",
        "title": "门店状态"
      },
      {
        "name": "storeCustomerId",
        "title": "门店编码"
      },
      {
        "name": "storeMobile",
        "title": "门店账号"
      },
      {
        "name": "storeName",
        "title": "门店名称"
      },
      {
        "name": "province",
        "title": "省"
      },
      {
        "name": "city",
        "title": "市"
      },
      {
        "name": "county",
        "title": "县"
      },
      {
        "name": "town",
        "title": "乡/镇"
      },
      {
        "name": "village",
        "title": "村"
      },
      {
        "name": "storeAddress",
        "title": "地址"
      },
      {
        "name": "shopkeeper",
        "title": "联系人"
      },
      {
        "name": "payTypeDesc",
        "title": "支付方式"
      },
      {
        "name": "",
        "title": "操作",
        "type": "button",
        "buttons": [
          {
            "name": "查看",
            "icon": null,
            "event": {
              "type": "router",
              "params": [
                "id"
              ],
              "static": {
                "method": "detail"
              },
              "path": "",
              "configKey": "/template/module/store/store_user_list_view.json"
            }
          },
          {
            "name": "编辑",
            "icon": null,
            "event": {
              "type": "router",
              "params": [
                "id"
              ],
              "static": {
                "method": "update"
              },
              "path": "",
              "configKey": "/template/module/store/store_user_list_edit.json"
            }
          }
        ]
      }
    ],
    "params": [

      {
        "name": "storeStatus",
        "inputModel": "select",
        "title": "状态",
        "value": "",
        "placeholder": "请选择状态",
        "options":[
          {
            "label":"有效",
            "value":1
          },
          {
            "label":"无效",
            "value":2
          }],
        "isHidden": false
      },
      {
        "name": "storeMobile",
        "inputModel": "input",
        "title": "门店账户",
        "value": "",
        "placeholder": "",
        "isHidden": false
      },{
        "name": "storeName",
        "inputModel": "input",
        "title": "门店名称",
        "value": "",
        "placeholder": "",
        "isHidden": false
      },{
        "name": "regionCode",
        "inputModel": "inputDia",
        "title": "区域",
        "placeholder": "请选择门店所属区域",
        "path": "",
        "configKey": "/template/module/system/system_region_list_radio_win.json",
        "isHidden": false
      }
    ]
  }
}
