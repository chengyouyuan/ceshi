{
  "meta": {
    "ajaxType": "get",
    "path": "/pay/verify/accountingDetailList",
    "type": "report",
    "title": "核销明细列表",
    "buttons": [
      {
        "name": "查询",
        "event": {
          "type": "submit",
          "ajaxType": "get",
          "path": "/pay/verify/accountingDetailList"
        }
      },
      {
        "name": "重置",
        "event": {
          "type": "reset"
        }
      },
      {
        "name": "恢复费用核销",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id" ],
          "path": "/pay/verify/accountingDetailRestore",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "暂缓",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id" ],
          "path": "/pay/verify/accountingDetailPause",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "确认结算",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id" ],
          "path": "/pay/verify/verifyByDetail",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      }
    ],
    "columns": [
      {
        "type": "selection",
        "name": "id"
      },
      {
        "name": "verifyStatus",
        "title": "结算状态"
      },
      {
        "name": "storeName",
        "title": "门店名称"
      },
      {
        "name": "storeId",
        "title": "门店ID"
      },
      {
        "name": "recordedTime",
        "title": "订单完成时间"
      },
      {
        "name": "recordedTime",
        "title": "订单入账时间"
      },
      {
        "name": "thirdPartyVerifyStatusName",
        "title": "微信入账"
      },
      {
        "name": "orderNo",
        "title": "关联订单"
      },
      {
        "name": "detailTypeName",
        "title": "费用类型"
      },
      {
        "name": "detailMoney",
        "title": "金额"
      },
      {
        "name": "verifyTime",
        "title": "实际结算时间"
      },
      {
        "name": "operatedByName",
        "title": "操作人"
      }
    ],
    "params": [
      {
        "name": "storeId",
        "inputModel": "inputDia",
        "title": "门店",
        "configKey": "/template/module/order/order_store_list_win.json"
      },
      {
        "name": "orderNo",
        "inputModel": "input",
        "title": "订单号"
      },
      {
        "name": "recordedDateStart",
        "inputModel": "single-date-picker",
        "title": "费用入账时间"
      },
      {
        "name": "recordedDateEnd",
        "inputModel": "single-date-picker",
        "title": "费用入账时间"
      },
      {
        "name": "verifyDateStart",
        "inputModel": "single-date-picker",
        "title": "结算时间"
      },
      {
        "name": "verifyDateEnd",
        "inputModel": "single-date-picker",
        "title": "结算时间"
      }
    ]
  }
}
