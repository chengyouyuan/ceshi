{
  "meta": {
    "type": "base-form",
    "path": "",
    "id": "system_user_list_add",
    "title": "成员添加",
    "buttons": [
      {
        "name": "保存",
        "icon": null,
        "event": {
          "type": "submit",
          "path": "/user/add",
          "callback": {
            "refresh": false
          }
        }
      },
      {
        "name": "取消",
        "icon": null,
        "event": {
          "type": "cancel",
          "callback": {
            "refresh": true
          }
        }
      }
    ],
    "items": [
      {
        "name": "username",
        "inputModel": "input",
        "title": "姓名",
        "value": "",
        "placeholder": "请输入姓名",
        "isHidden": false,
        "validate": {
          "required": true,
          "maxLength": {
            "value": 25,
            "message": "允许最大长度为25个字符"
          },
          "minLength": {
            "value": 2,
            "message": "最短长度为2个字符"
          }
        }
      },
      {
        "name": "account",
        "inputModel": "input",
        "title": "账号",
        "value": "",
        "placeholder": "请输入账号",
        "isHidden": false,
        "validate": {
          "required": true,
          "maxLength": {
            "value": 50,
            "message": "允许最大长度为50个字符"
          },
          "minLength": {
            "value": 3,
            "message": "最短长度为3个字符"
          },
          "alphaNum": true
        }
      },
      {
        "name": "mobile",
        "inputModel": "input",
        "title": "手机号",
        "value": "",
        "placeholder": "请输入手机号",
        "isHidden": false,
        "validate": {
          "regex": {
            "value": "/^((1[358][0-9])|(14[57])|(17[0678])|(19[7]))\\d{8}$/",
            "message": "手机号码格式错误"
          }
        }
      },
      {
        "name": "roleId",
        "inputModel": "inputDia",
        "title": "权限组",
        "placeholder": "请选择权限组",
        "isHidden": false,
        "path": "",
        "configKey": "/template/module/system/system_role_list_win.json",
        "validate": {
          "required": true
        }
      },
      {
        "name": "regionCode",
        "inputModel": "inputDia",
        "title": "地理区域",
        "placeholder": "请选择地理区域",
        "isHidden": false,
        "path": "",
        "configKey": "/template/module/system/system_region_list_radio_win.json"
      }
    ]
  }
}
