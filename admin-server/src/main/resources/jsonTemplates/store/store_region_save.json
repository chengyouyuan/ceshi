    "items": [
      {
        "name": "areaCode",
        "inputModel": "inputDia",
        "title": "区域",
        "placeholder": "请选择门店所属区域",
        "path": "",
        "configKey": "/template/module/system/system_region_list_radio_win.json",
        "isHidden": false,
        "validate": {
          "required": true
        }
      }
    ]
  }
}
