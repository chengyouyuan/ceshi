{
  "meta": {
    "path": "/store/1021/v1/getStoreInfoById/{id}",
    "type": "base-form",
    "id": "store_user_list_view",
    "code": "store_user_list_view",
    "title": "门店编辑",
    "buttons": [
      {
        "name": "保存",
        "icon": null,
        "event": {
          "type": "submit",
          "path": "/store/1022/v1/modifyStoreInfo",
          "callback": {
            "refresh": false
          }
        }
      },
      {
        "name": "取消",
        "icon": null,
        "event": {
          "type": "cancel",
          "callback": {
            "refresh": true
          }
        }
      }
    ],
    "items": [
      {
        "name": "storeCustomerId",
        "inputModel": "input",
        "title": "门店编码",
        "value": "",
        "placeholder": "请填写门店编码",
        "isHidden": false
      },
      {
        "name": "storeMobile",
        "title": "门店账号",
        "inputModel": "input",
        "value": "",
        "placeholder": "请填写门店账号",
        "isHidden": false
      },
      {
        "name": "storeName",
        "title": "门店名称",
        "inputModel": "input",
        "value": "",
        "placeholder": "请填写门店名称",
        "isHidden": false
      },
      {
        "name": "province",
        "title": "省",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "city",
        "title": "市",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "county",
        "title": "县",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "town",
        "title": "乡/镇",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "village",
        "title": "村",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "storeAddress",
        "title": "地址",
        "inputModel": "input",
        "value": "",
        "placeholder": "请填写地址",
        "isHidden": false
      },
      {
        "name": "shopkeeper",
        "title": "联系人",
        "inputModel": "input",
        "value": "",
        "isHidden": false
      },
      {
        "name": "payType",
        "title": "支付方式",
        "inputModel": "input",
        "value": "",
        "placeholder": "请选择支付方式",
        "isHidden": false
      },
      {
        "name": "storeStatus",
        "inputModel": "select",
        "title": "门店状态",
        "placeholder": "",
        "isHidden": false,
        "options": [{
          "value": "0",
          "label": "未开店"
        },
          {
            "value": "1",
            "label": "有效"
          },
          {
            "label":"无效",
            "value":2
          }
        ]
      }
    ]
  }
}
