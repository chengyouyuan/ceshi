    "items": [
      {
        "name": "username",
        "inputModel": "input",
        "title": "姓名",
        "value": "",
        "placeholder": "请输入姓名",
        "isHidden": false
      },
      {
        "name": "account",
        "inputModel": "input",
        "title": "账号",
        "value": "",
        "placeholder": "请输入账号",
        "isHidden": false
      },
      {
        "name": "mobile",
        "inputModel": "input",
        "title": "手机号",
        "value": "",
        "placeholder": "请输入手机号",
        "isHidden": false
      },
      {
        "name": "roleId",
        "inputModel": "inputDia",
        "title": "权限组",
        "placeholder": "请选择权限组",
        "isHidden": false,
        "path": "",
        "configKey": "/template/module/system/system_role_list_win.json"
      },
      {
        "name": "regionCode",
        "inputModel": "inputDia",
        "title": "地理区域",
        "placeholder": "请选择地理区域",
        "isHidden": false,
        "path": "",
        "configKey": "/template/module/system/system_region_list_radio_win.json"
      }
    ]
  }
}
