{
  "meta": {
    "code": "storeWithdrawList",
    "ajaxType": "get",
    "path": "/pay/verify/storeWithdrawList",
    "type": "report",
    "title": "提款申请列表",
    "buttons": [
      {
        "name": "查询",
        "event": {
          "type": "submit",
          "ajaxType": "get",
          "path": "/pay/verify/storeWithdrawList"
        }
      },
      {
        "name": "重置",
        "event": {
          "type": "reset"
        }
      },
      {
        "name": "确认提款",
        "event": {
          "type": "batch",
          "ajaxType": "post",
          "params": [ "id", "storeId", "auditStatus", "callbackStatus" ],
          "path": "/pay/verify/approveStoreWithdraw",
          "confirm": {
            "title": "提示",
            "message": "确认继续操作吗"
          }
        }
      },
      {
        "name": "导出Excel",
        "event": {
          "type": "export",
          "ajaxType": "post",
          "path": "/pay/verify/storeWithdrawalsListExport"
        }
      }
    ],
    "columns": [
      {
        "type": "selection",
        "name": "id"
      },
      {
        "name": "storeName",
        "title": "提款门店"
      },
      {
        "name": "totalFee",
        "title": "提款金额"
      },
      {
        "name": "flowDirectionName",
        "title": "流向"
      },
      {
        "name": "cmmsAmt",
        "title": "手续费"
      },
      {
        "name": "realFee",
        "title": "实际到款金额"
      },
      {
        "name": "paymentAccount",
        "title": "付款账户"
      },
      {
        "name": "name",
        "title": "提款人"
      },
      {
        "name": "mobile",
        "title": "提款手机号"
      },
      {
        "name": "timeEnd",
        "title": "提款时间"
      },
      {
        "name": "auditStatusName",
        "title": "审核状态"
      },
      {
        "name": "callbackStatusName",
        "title": "提款状态"
      },
      {
        "name": "callbackReason",
        "title": "备注"
      },
      {
        "name": "operatedByName",
        "title": "操作人员"
      }
    ],
    "params": [
      {
        "name": "storeId",
        "inputModel": "inputDia",
        "title": "门店",
        "configKey": "/template/module/order/order_store_list_win.json"
      },
      {
        "name": "withdrawalsDate",
        "inputModel": "double-date-picker",
        "title": "提款时间",
        "value": {}
      },
      {
        "name": "mobile",
        "inputModel": "input",
        "title": "联系电话"
      },
      {
        "name": "totalFee",
        "inputModel": "range",
        "title": "提款金额",
        "value": {}
      },
      {
        "name": "auditStatus",
        "inputModel": "select",
        "title": "审核状态",
        "options": [ {
          "label": "未审核",
          "value": "0"
        }, {
          "label": "审核通过",
          "value": "1"
        } ]
      },
      {
        "name": "callbackStatus",
        "inputModel": "select",
        "title": "提款状态",
        "options": [ {
          "label": "申请中",
          "value": "0"
        }, {
          "label": "处理中",
          "value": "4"
        }, {
          "label": "提现成功",
          "value": "1"
        }, {
          "label": "提款失败",
          "value": "2"
        }, {
          "label": "无效",
          "value": "3"
        } ]
      }
    ]
  }
}
